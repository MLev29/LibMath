# Line endings
*.c		text eof=lf
*.cpp	text eof=lf
*.h		text eof=lf
*.hpp	text eof=lf

*.gitattributes	text eof=lf
.gitignore		text eof=lf
*.md			text eof=lf

# Git Files
*.gitattributes	text
.gitignore		text
*.md			text diff=markdown

# Source Files
*.c		text diff=cpp
*.cpp	text diff=cpp
*.h		text diff=cpp
*.hpp	text diff=cpp


# Compiled Object files
*.slo	binary
*.lo	binary
*.o		binary
*.obj	binary

# Precompiled Headers
*.pch	binary

# Compiled Static libraries
*.lib	binary

# Executables
*.exe	binary